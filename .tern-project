{
  "ecmaVersion": "7",
  "libs": [
    "browser"
  ],
  "loadEagerly": [
    "**/*.js",
    "**/*.html",
    "**/*.vue"
  ],
  "dontLoad": [
    "node_modules/**"
  ],
  "plugins": {
    "doc_comment": true,
    "es_modules": {},
    "node": {
      "dontLoad": "",
      "load": "",
      "modules": ""
    },
    "node_resolve": {},
    "modules": {
      "dontLoad": "",
      "load": "",
      "modules": ""
    }
  }
}